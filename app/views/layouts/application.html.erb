               <li class="dropdown">
                <a href="#" class="dropdown-toggle" data-toggle="dropdown">Nos services <b class="caret"></b></a>
                <ul class="dropdown-menu">
                  <li><%= link_to "Rénovation", new_estimate_path, :class => "link_about"%></li>
                  <li><%= link_to "Plomberie", formulaireplomberie_path, :class => "link_about"%></li>
                  <li><%= link_to "Bricolage", formulairebrico_path, :class => "link_about"%></li>
                  <li><%= link_to "Electroménager", formulaireelectromenager_path, :class => "link_about"%></li>
                </ul>
              </li>
              <!--<li><%= link_to "Solutions pour entreprises", solutionsentreprises_path, :class => "link_about" %></li>-->
               <li><%= link_to "Mode d'emploi", mode_emploi_path, :class => "link_about" %></li>
